        "next-compose-plugins": "^2.2.1",
        "next-i18next": "^10.2.0",
        "next-seo": "^5.0.0",
        "prettier": "^2.4.1",
        "react": "17.0.2",
        "react-bootstrap": "^2.1.1",
        "react-dom": "17.0.2",
        "react-redux": "^7.2.4",
        "react-use": "^17.3.2",
        "sass": "^1.42.1"
    },
    "devDependencies": {
        "@svgr/cli": "^5.5.0",
