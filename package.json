        "eslint": "^7.15.0",
        "husky": "^7.0.2",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-prettier": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "isomorphic-unfetch": "^3.1.0",
        "jest": "^26.6.3",
        "lint-staged": "^12.3.2",
        "nock": "^13.0.6",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
